<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/qr_nav_graph"
    app:startDestination="@id/addEditFarmingRecordQrFragment">

    <fragment
        android:id="@+id/qrTestFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.QrTestFragment"
        android:label="QrTestFragment"
        tools:layout="@layout/farming_record_qr_home_fragment" />

    <fragment
        android:id="@+id/farmingRecordQrHomeFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.FarmingRecordQrHomeFragment"
        android:label="FarmingRecordQrHomeFragment"
        tools:layout="@layout/farming_record_qr_home_fragment">
        <action
            android:id="@+id/action_farmingRecordQrHomeFragment_to_addEditFarmingRecordQrFragment"
            app:destination="@id/addEditFarmingRecordQrFragment" />
        <action
            android:id="@+id/action_farmingRecordQrHomeFragment_to_qrFullScreenFragment"
            app:destination="@id/qrFullScreenFragment" />
        <action
            android:id="@+id/action_farmingRecordQrHomeFragment_to_farmingRecordQrOrderStatusFragment"
            app:destination="@id/farmingRecordQrOrderStatusFragment" />
    </fragment>

    <fragment
        android:id="@+id/addEditFarmingRecordQrFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.AddEditFarmingRecordQrFragment"
        android:label="addEditFarmingRecordQrFragment"
        tools:layout="@layout/add_edit_farming_record_qr_fragment">
        <action
            android:id="@+id/action_addEditFarmingRecordQrFragment_to_farmingRecordQrOrderSuccessFragment"
            app:destination="@id/farmingRecordQrOrderSuccessFragment" />
        <action
            android:id="@+id/action_addEditFarmingRecordQrFragment_to_farmPickerDialogFragment"
            app:destination="@id/farmPickerDialogFragment" />
        <argument
            android:name="qr_edit_arg"
            app:argType="greenway_myanmar.org.features.farmingrecord.qr.presentation.model.args.UiQrOrderEditArg"
            android:defaultValue="@null"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/farmingRecordQrOrderSuccessFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.FarmingRecordQrOrderSuccessFragment"
        android:label="FarmingRecordQrHomeFragment"
        tools:layout="@layout/farming_record_qr_order_success_fragment">
        <argument
            android:name="quantity"
            app:argType="integer" />
    </fragment>

    <fragment
        android:id="@+id/farmingRecordQrOrderStatusFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.QrOrderStatusFragment"
        android:label="FarmingRecordQrOrderStatusFragment"
        tools:layout="@layout/farming_record_qr_order_status_fragment" >
        <action
            android:id="@+id/action_farmingRecordQrOrderStatusFragment_to_addEditFarmingRecordQrFragment"
            app:destination="@id/addEditFarmingRecordQrFragment" />
        <argument
            android:name="order_status_arg"
            app:argType="greenway_myanmar.org.features.farmingrecord.qr.presentation.model.args.UiQrOrderStatusArg" />
    </fragment>
    <fragment
        android:id="@+id/qrFullScreenFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.QrFullScreenFragment"
        android:label="QrFullScreenFragment"
        tools:layout="@layout/qr_full_screen_fragment">
        <argument
            android:name="qr_data"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/farmPickerDialogFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.pickers.FarmPickerFragment"
        android:label="FarmPickerDialogFragment" />

</navigation>