<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/qr_nav_graph"
    app:startDestination="@id/addEditFarmingRecordQrFragment">

    <fragment
        android:id="@+id/farmingRecordQrHomeFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.FarmingRecordQrHomeFragment"
        android:label="FarmingRecordQrHomeFragment"
        tools:layout="@layout/farming_record_qr_home_fragment">
        <action
            android:id="@+id/action_farmingRecordQrHomeFragment_to_addEditFarmingRecordQrFragment"
            app:destination="@id/addEditFarmingRecordQrFragment" />
    </fragment>

    <fragment
        android:id="@+id/addEditFarmingRecordQrFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.AddEditFarmingRecordQrFragment"
        android:label="addEditFarmingRecordQrFragment"
        tools:layout="@layout/add_edit_farming_record_qr_fragment">
        <action
            android:id="@+id/action_addEditFarmingRecordQrFragment_to_farmingRecordQrOrderSuccessFragment"
            app:destination="@id/farmingRecordQrOrderSuccessFragment" />
    </fragment>

    <fragment
        android:id="@+id/farmingRecordQrOrderSuccessFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.FarmingRecordQrOrderSuccessFragment"
        android:label="FarmingRecordQrHomeFragment"
        tools:layout="@layout/farming_record_qr_order_success_fragment" >
        <action
            android:id="@+id/action_farmingRecordQrOrderSuccessFragment_to_farmingRecordQrOrderStatusFragment"
            app:destination="@id/farmingRecordQrOrderStatusFragment" />
    </fragment>

    <fragment
        android:id="@+id/farmingRecordQrOrderStatusFragment"
        android:name="greenway_myanmar.org.features.farmingrecord.qr.presentation.FarmingRecordQrOrderStatusFragment"
        android:label="FarmingRecordQrOrderStatusFragment"
        tools:layout="@layout/farming_record_qr_order_status_fragment" />

</navigation>