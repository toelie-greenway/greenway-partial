    <style name="ThemeOverlay.GreenWay.RadioButton.Primary" parent="ThemeOverlay.MaterialComponents">
        <item name="colorControlActivated">?colorPrimary</item>
    </style>

    <style name="ThemeOverlay.GreenWay.NavigationView" parent="">
