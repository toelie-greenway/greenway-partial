    </style>

    <!-- Switch -->
    <style name="Widget.GreenWay.Switch.Primary" parent="Widget.MaterialComponents.CompoundButton.Switch">
        <item name="materialThemeOverlay">@style/ThemeOverlay.GreenWay.Switch.Primary</item>
    </style>

    <style name="ThemeOverlay.GreenWay.Switch.Primary" parent="">
        <item name="colorOnSurface">@color/app_inactive_icon</item>
        <item name="colorSecondary">?colorPrimary</item>
    </style>


