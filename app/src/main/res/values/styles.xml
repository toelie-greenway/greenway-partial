<resources xmlns:tools="http://schemas.android.com/tools">

    <!--Components-->
    <style name="Theme.GreenWayPartial.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.GreenWayPartial.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <!--Bottom Sheets-->
    <style name="ThemeOverlay.GreenWay.BottomSheetDialog" parent="ThemeOverlay.MaterialComponents.Dialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/Animation.GreenWay.BottomSheet.Modal</item>
        <item name="android:statusBarColor" tools:targetApi="lollipop">@android:color/transparent
        </item>
        <item name="bottomSheetStyle">@style/Widget.GreenWay.BottomSheet.Modal</item>
        <item name="android:navigationBarColor" tools:targetApi="lollipop">?colorSurface</item>
        <item name="android:navigationBarDividerColor" tools:ignore="NewApi">
            @android:color/transparent
        </item>
    </style>

    <style name="Widget.GreenWay.BottomSheet.Modal" parent="Widget.MaterialComponents.BottomSheet.Modal">
        <item name="shapeAppearance">?shapeAppearanceLargeComponent</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.GreenWay.BottomSheet
        </item>
    </style>

    <!--Navigation Drawer-->
    <style name="Widget.GreenWay.NavigationView" parent="Widget.MaterialComponents.NavigationView">
        <item name="itemTextAppearance">?textAppearanceBody2</item>
    </style>

    <!-- Toolbar -->
    <style name="Widget.GreenWay.Toolbar.Primary" parent="Widget.MaterialComponents.Toolbar.Primary"></style>

    <style name="Widget.GreenWay.Toolbar.Primary.NoElevation" parent="Widget.MaterialComponents.Toolbar.Primary">
        <item name="android:elevation" tools:ignore="NewApi">0dp</item>
    </style>

    <style name="Widget.GreenWay.Toolbar.Surface" parent="Widget.MaterialComponents.Toolbar.Surface">

    </style>


    <style name="Widget.GreenWay.Toolbar.Button.Navigation.Primary" parent="Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="tint">?colorOnPrimary</item>
    </style>

    <!-- TabLayout -->
    <style name="Widget.GreenWay.TabLayout.Colored" parent="Widget.MaterialComponents.TabLayout.Colored">
        <item name="tabIndicator">@drawable/app_tabs_on_primary_rounded_line_indicator</item>
    </style>

    <style name="Widget.GreenWay.TabLayout.PrimarySurface" parent="Widget.MaterialComponents.TabLayout.PrimarySurface">
        <item name="tabIndicator">@drawable/app_tabs_primary_rounded_line_indicator</item>
    </style>

    <style name="Widget.GreenWay.TabLayout.Surface" parent="Widget.MaterialComponents.TabLayout">
        <item name="tabIndicator">@drawable/app_tabs_primary_rounded_line_indicator</item>
    </style>

    <style name="Widget.GreenWay.TabLayout.Switch" parent="Widget.MaterialComponents.TabLayout">
        <item name="tabBackground">@drawable/app_tab_item_switch_bg_selector</item>
        <item name="tabIndicatorHeight">0dp</item>
        <item name="tabTextColor">@color/app_tab_switch_text_selector</item>
        <item name="tabTextAppearance">@style/TextAppearance.GreenWay.TabLayout</item>
        <item name="tabUnboundedRipple">false</item>
        <item name="tabRippleColor">@color/app_transparent</item>
        <item name="android:background">@drawable/app_tab_switch_bg</item>
    </style>

    <style name="TextAppearance.GreenWay.TabLayout" parent="TextAppearance.MaterialComponents.Button">
        <item name="fontFamily">@font/noto_sans_myanmar_medium_500</item>
        <item name="android:fontFamily">@font/noto_sans_myanmar_medium_500</item>
        <item name="android:textSize">16sp</item>
    </style>

    <!-- Buttons -->
    <style name="Widget.GreenWay.Button" parent="Widget.MaterialComponents.Button" />

    <style name="Widget.GreenWay.Button.TextButton.Large">
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="cornerRadius">8dp</item>
    </style>

    <style name="Widget.GreenWay.Button.Small" parent="Widget.GreenWay.Button">
        <item name="android:paddingStart">@dimen/spacing_micro</item>
        <item name="android:paddingLeft">@dimen/spacing_micro</item>
        <item name="android:paddingTop">@dimen/spacing_nano</item>
        <item name="android:paddingEnd">@dimen/spacing_normal</item>
        <item name="android:paddingRight">@dimen/spacing_normal</item>
        <item name="android:paddingBottom">@dimen/spacing_nano</item>
    </style>

    <style name="Widget.GreenWay.Button.Primary" parent="Widget.MaterialComponents.Button">
        <item name="android:textColor">?colorOnPrimary</item>
        <item name="backgroundTint">@drawable/app_btn_bg_primary_selector</item>
    </style>

    <style name="Widget.GreenWay.Button.Secondary" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@drawable/app_btn_bg_secondary_selector</item>
        <item name="android:textColor">?colorOnPrimary</item>
    </style>

    <style name="Widget.GreenWay2.Button.Secondary" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@drawable/app_btn_bg_secondary_selector</item>
        <item name="android:textColor">?colorOnSecondary</item>
    </style>

    <style name="Widget.GreenWay2.Button.TextButton" parent="Widget.MaterialComponents.Button.TextButton" />

    <style name="Widget.GreenWay2.Button.Negative" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:textColor">?colorOnSecondary</item>
    </style>

    <style name="Widget.GreenWay.Button.SecondaryVariant" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@drawable/app_btn_bg_secondary_variant_selector</item>
        <item name="android:textColor">?colorOnPrimary</item>
    </style>

    <style name="Widget.GreenWay.Button.TextButton" parent="Widget.MaterialComponents.Button.TextButton">

    </style>

    <style name="Widget.GreenWay.Button.TextButton.Negative" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:textColor">?colorOnSurface</item>
        <item name="android:alpha">?alphaEmphasisMedium</item>
    </style>

    <style name="Widget.GreenWay.Button.OutlinedButton" parent="Widget.MaterialComponents.Button.OutlinedButton"></style>

    <style name="Widget.GreenWay.Button.OutlinedButton.Thin" parent="Widget.GreenWay.Button.OutlinedButton">
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="cornerRadius">20dp</item>
    </style>

    <style name="Widget.GreenWay.Button.OutlinedButton.Icon.Blue" parent="Widget.MaterialComponents.Button.OutlinedButton.Icon">
        <item name="iconTint">@color/app_blue</item>
        <item name="android:textColor">@color/app_blue</item>
        <item name="strokeColor">#e2e4ed</item>
        <item name="iconGravity">textStart</item>
    </style>

    <style name="Widget.GreenWay.Button.TextButton.Icon.Blue" parent="Widget.MaterialComponents.Button.TextButton.Icon">
        <item name="iconTint">@color/app_blue</item>
        <item name="android:textColor">@color/app_blue</item>
        <item name="strokeColor">#e2e4ed</item>
        <item name="iconGravity">textStart</item>
    </style>

    <style name="Widget.GreenWay.Button.UnelevatedButton.Icon" parent="Widget.MaterialComponents.Button.UnelevatedButton.Icon" />

    <style name="Widget.GreenWay.Button.UnelevatedButton" parent="Widget.MaterialComponents.Button.UnelevatedButton" />

    <style name="Widget.GreenWay.Button.UnelevatedButton.Rounded" parent="Widget.GreenWay.Button.UnelevatedButton">
        <item name="materialThemeOverlay">@style/ThemeOverlay.GreenWay.Button</item>
        <item name="shapeAppearance">@style/ShapeAppearance.GreenWay.Button</item>
    </style>

    <style name="Widget.GreenWay.Button.UnelevatedButton.Rounded.Large">
        <item name="android:insetTop">2dp</item>
        <item name="android:insetBottom">2dp</item>
    </style>

    <style name="ThemeOverlay.GreenWay.Button" parent="">
        <!--        <item name="colorPrimary">?colorPrimary</item>-->
    </style>

    <style name="Widget.GreenWay.Button.UnelevatedButton.Large">
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="cornerRadius">8dp</item>
    </style>

    <!-- Unused -->
    <style name="ThemeOverlay.GreenWay.Button.Primary.Disabled" parent="ThemeOverlay.MaterialComponents">
