<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:parentTag="android.widget.LinearLayout">

    <greenway_myanmar.org.ui.widget.GreenWayButton
        android:id="@+id/more_button"
        style="@style/Widget.GreenWay.Button.UnelevatedButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:fontFamily="@font/noto_sans_myanmar_medium_500"
        android:text="@string/tag_action_more_thread_post"
        android:textColor="#202020"
        android:textSize="16sp"
        app:backgroundTint="#F0F0F0"
        app:cornerRadius="56dp"
        app:fontFamily="@font/noto_sans_myanmar_medium_500"
        app:icon="@drawable/ic_chevron_right_black_24dp"
        app:iconGravity="end"
        app:iconTint="#202020" />

</merge>